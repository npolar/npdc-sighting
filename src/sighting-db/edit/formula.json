{
    "fieldsets": [{
            "title": "Travel/Trip",
            "fields": [
                         {
                            "id": "title",
                            "title": "name travel/trip/expedition"
                        },
                        {
                            "id": "contact_info",
                            "title": "contact information - addess, additional email etc"
                        },
                        "organisation",
                        {
                            "id": "platform",
                            "title": "platform - boat, skis, kayak, scooter, plane etc"
                        },
                        {
                            "id": "platform_comment",
                            "title": "platform comment - vessel name etc"
                        },
                        {
                            "id": "start_date",
                            "title": "start date"
                        },
                        {
                            "id": "end_date",
                            "title": "end date"
                        },
                        {
                            "id": "href",
                            "title": "link to NPI's expedition database (NPI staff only)"

      }]
},
                    {
            "title": "Observation",
            "fields": [
                {
                    "id": "species",
                    "description": "what kind of animal did you see?"
                },
                 {
                    "id": "habitat",
                    "title": "habitat"
                },
                {
                    "id": "adult_m",
                    "title": "number of male adults"
                },
                {
                    "id": "adult_f",
                    "title": "number of female adults"
                },
                {
                    "id": "adult",
                    "title": "number of adults when sex is unknown",
                    "description": "number of adults where sex is unknown"
                },
                {
                    "id": "sub_adult",
                    "title": "number of young adults, not included previously",
                    "description": "number of young adults, not included previously"
                },
                {
                    "id": "cub_calf_pup",
                    "title": "number of cubs/calfs/pups"
                },
                 {
                    "id": "bear_cubs",
                    "title": "how old were the polar bear cubs?",
                    "description": "one-year old, two-year old etc"
                },
                {
                    "id": "polar_bear_condition",
                    "title": "what was the condition of the polar bear(s)?",
                    "description": "describe fat on polar bear - 1 is very skinny, 5 is fat"
                },
                {
                    "id": "polar_bear_den",
                    "title": "did you see a polar bear den?",
                    "description": "did you see a polar bear den?"
                },
                {
                    "id": "unidentified",
                    "title":"number of unidentified",
                    "description": "don't have specific information about adult, juvenile, subadult."
                },
                {
                    "id": "dead_alive",
                    "title": "observed dead or alive?",
                    "description": "Observed dead or alive?"
                },
                {
                    "id": "total",
                    "title": "total count"
                }
            ]
        },
        {
            "title": "Time/Location",
            "fields": [
                 {
                    "id": "event_date",
                    "title": "observation date",
                    "description": "Date of observation"
                },
                {
                    "id": "latitude",
                    "description": "Perferably in decimal degrees"
                },
                {
                    "id": "longitude",
                    "description": "Preferably in decimal degrees"
                },
                {
                    "id": "@placename",
                    "title": "placename",
                    "description": "Observation's placename"
                },
                {
                    "id": "placename_comment",
                    "title": "place comment",
                    "description": "place comment"
                }
            ]
        },
{
